
server {
    listen 443 ssl;
    
    access_log on;
    server_name  "";
        ssl_certificate      /etc/nginx/ssl/nginx.crt; # fügt Zertifikat hinzu
        ssl_certificate_key /etc/nginx/ssl/nginx.key; # fügt privaten Schlüssel hinzu
        
         ssl on;
    ssl_session_cache  builtin:1000  shared:SSL:10m;
    ssl_protocols  TLSv1 TLSv1.1 TLSv1.2;
    ssl_ciphers HIGH:!aNULL:!eNULL:!EXPORT:!CAMELLIA:!DES:!MD5:!PSK:!RC4;
    ssl_prefer_server_ciphers on;

        location ^~ /securejenkins/  {
        
        proxy_pass         https://jenkins-fat:8080;  
        include /etc/nginx/conf.d/proxy-settings.conf;
    }

}
